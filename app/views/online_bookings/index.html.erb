<p id="notice"><%= notice %></p>

<h1>Listing Online Bookings</h1>

<table>
  <thead>
    <tr>
      <th>Movie</th>
      <th>Booking</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @online_bookings.each do |online_booking| %>
      <tr>
        <td><%= online_booking.movie_id %></td>
        <td><%= online_booking.booking_id %></td>
        <td><%= link_to 'Show', online_booking %></td>
        <td><%= link_to 'Edit', edit_online_booking_path(online_booking) %></td>
        <td><%= link_to 'Destroy', online_booking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Online booking', new_online_booking_path %>
